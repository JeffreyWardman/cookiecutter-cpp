{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include",
        "/usr/local/include",
        "/usr/include/c++/11",
        "/usr/include/x86_64-linux-gnu/c++/11",
        "${workspaceFolder}/target/build/vcpkg_installed/x64-linux/**",
        "${vcpkgRoot}/x64-linux/include"
      ],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
      "cStandard": "c23",
      "cppStandard": "c++20",
      "compilerPath": "/usr/bin/g++",
      "intelliSenseMode": "${default}",
      "configurationProvider": "ms-vscode.cmake-tools",
      "compileCommands": "${workspaceFolder}/target/build/compile_commands.json"
    },
    {
      "name": "Mac",
      "includePath": ["${workspaceFolder}/**"],
      "defines": [],
      "macFrameworkPath": [
        "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
      ],
      "cStandard": "c23",
      "cppStandard": "c++20",
      "intelliSenseMode": "macos-clang-arm64",
      "configurationProvider": "ms-vscode.cmake-tools",
      "compilerPath": "${workspaceFolder}/target/build/compile_commands.json"
    }
  ],
  "version": 4
}
